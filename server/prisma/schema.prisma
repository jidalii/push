generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Records {
  id            Int      @id @default(autoincrement())
  depositor     String
  beneficiary   String
  hashCondition String
  rewards       Float
  activity      Int
  numTimes      Int
  period        Int
  condition     Json
  startTime     DateTime
  endTime       DateTime
  // claimed       Boolean
}
